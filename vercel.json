{
    "version": 2,
    "builds": [
        { "src": "api/webhook.js", "use": "@vercel/node" }
    ],
    "redirects": [
      { "source": "/api/(.*)", "destination": "/api/webhook.js", "statusCode": 200 },
      { "source": "/", "destination": "public/index.html", "statusCode": 200 },
      { "source": "/index", "destination": "public/index.html", "statusCode": 200 }
    ],
    "cleanUrls": true
  }
  